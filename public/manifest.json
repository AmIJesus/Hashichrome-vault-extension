{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Cheeki Breeki Extension",
  "short_name": "Cheeki Breeki Extension",
  "description": "Cheeki Breeki Extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Cheeki Breeki Extension"
  },
  "permissions": [
    "background",
    "activeTab",
    "storage",
    "notifications",
    "debugger",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "256": "react256.png"
  }
}